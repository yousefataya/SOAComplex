package com.dimensions.mw.utils.errorManagement.inMemoryData;

public class ErrorDefinitionBean {
    private String errorCode;
    private String errorBussinessCode;
    private String errorBussinessMessage;
    private String errorTechCode;
    private String errorTechMessage;
    private String errorTechDetails;

    public void setErrorCode(String errorCode) {
        this.errorCode = errorCode;
    }

    public String getErrorCode() {
        return this.errorCode;
    }

    public void setErrorBussinessCode(String errorBussinessCode) {
        this.errorBussinessCode = errorBussinessCode;
    }

    public String getErrorBussinessCode() {
        return this.errorBussinessCode;
    }

    public void setErrorBussinessMessage(String errorBussinessMessage) {
        this.errorBussinessMessage = errorBussinessMessage;
    }

    public String getErrorBussinessMessage() {
        return this.errorBussinessMessage;
    }

    public void setErrorTechCode(String errorTechCode) {
        this.errorTechCode = errorTechCode;
    }

    public String getErrorTechCode() {
        return this.errorTechCode;
    }

    public void setErrorTechMessage(String errorTechMessage) {
        this.errorTechMessage = errorTechMessage;
    }

    public String getErrorTechMessage() {
        return this.errorTechMessage;
    }

    public void setErrorTechDetails(String errorTechDetails) {
        this.errorTechDetails = errorTechDetails;
    }

    public String getErrorTechDetails() {
        return this.errorTechDetails;
    }
}

