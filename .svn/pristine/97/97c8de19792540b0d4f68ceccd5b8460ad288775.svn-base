#EmailTemplateQueries#
SQ-EmailDB-getEmailTemplateByID-01 = SELECT A.TEMP_ID, A.EMAIL_TO, A.EMAIL_CC, A.EMAIL_BCC, A.EMAIL_SUBJECT, A.EMAIL_BODY FROM IMD_OFM_EMAIL_TEMPLATE_DEF A WHERE A.TEMP_ID = ?
SQ-EmailDB-insertEmailLogData-01 = INSERT INTO LOG_OFM_EMAIL A (A.OFM_PROCESS_NAME, A.OFM_TRANSACTION_ID, A.EMAIL_TEMP_ID, A.EMAIL_TO, A.EMAIL_CC, A.EMAIL_BCC, A.EMAIL_SUBJECT, A.EMAIL_BODY, A.STATUS, A.EXECUTION_TIME, A.ERROR_MESSAGE) VALUES(?,?,?,?,?,?,?,?,?,?,?)
SQ-EmailDB-updateEmailLogData-01 = UPDATE LOG_OFM_EMAIL A SET A.STATUS = ?, A.ERROR_MESSAGE = ? WHERE A.OFM_PROCESS_NAME = ? AND A.OFM_TRANSACTION_ID = ?
#********************#
#mappingManagement#
SQ-MappingDB-loadMapping-01 =  SELECT A.KEY, A.VALUE, A.DESCRIPTION FROM IMD_OFM_MAPPING_DEF A
#*****************#
#ValidationManagement#
SQ-ValidationDB-loadValidationMapping-01 = SELECT A.SOURCE, A.PROCESS_NAME, A.ELEMENET_NAME, A.TARGET_VALIDATIONS FROM IMD_OFM_COMPOSITE_VALIDATE_DEF A
#********************#
#ErrorManagement#
SQ-ErrorDB-loadErrorDefinitionMap-01 =SELECT A.ERROR_CODE, A.ERROR_BUS_CODE, A.ERROR_BUS_MESSAGE,A.ERROR_TECH_CODE, A.ERROR_TECH_MESSAGE FROM IMD_OFM_PROCESS_ERROR_DEF A
SQ-ErrorDB-loadErrorCriteriaList-01 = SELECT A.ERROR_CODE, A.ERROR_STRING, A.SYSTEM, A.OPERATION,A.PROCESSNAME, ORDER_P FROM IMD_OFM_PROCESS_ERROR_CRT A ORDER BY ORDER_P ASC
#***************#
#loggerManagement#
SQ-LoggerDB-insertProcessInstance-01 =  INSERT INTO LOG_OFM_PROCESS_EXCEUTION (COMPOSITE_NAME, OFM_TRANSACTION_ID, SOURCE_APP_CODE, SOURCE_TRANSACTION_ID, SOURCE_APP_USERNAME, EXECUTION_INPUT, EXECUTION_STATUS, START_TIME_STAMP, COMPOSITE_TRANSACTION_ID, DOMAIN_NAME, MANAGED_SERVER_NAME, BUSINESS_KEY) VALUES(?,?,?,?,?,?,?,?,?,?,?,?)
SQ-LoggerDB-updateProcessInstance-01 =  UPDATE LOG_OFM_PROCESS_EXCEUTION SET  END_TIME_STAMP = ? , EXECUTION_OUTPUT = ? , EXECUTION_STATUS = ? WHERE (PART_DATE BETWEEN SYSDATE - #DAYNUM# AND SYSDATE) AND OFM_TRANSACTION_ID = ?
#****************#
#parameterManagement#
SQ-ParameterDB-loadParameters-01 =  SELECT A.PARAM_NAME, A.PARAM_VALUE, A.PARAM_DESC, A.PARAM_ATTR1,A.PARAM_ATTR2, A.PARAM_ATTR3, A.PARAM_DEF_VALUE FROM IMD_OFM_PARAM_DEF A
#*******************#
#compositeManagement#
SQ-CompositeDB-loadCompositeDefinition-01 = SELECT A.COMPOSITE_NAME, A.DB_LOG FROM IMD_OFM_COMPOSITE_DEF A
SQ-CompositeDB-loadCompositeDefinition-02 = SELECT A.COMPOSITE_NAME, A.SOURCE_SYSTEM, A.SOURCE_IP_ADDRESS, A.TOKEN, A.USER_NAME FROM IMD_OFM_COMPOSITE_AUTH_MTRX_DF A WHERE COMPOSITE_NAME = ?
#*******************#
#AttributeManagment#
SQ-AttributeDB-loadAttributes-01 = SELECT A.ATTRIBUTE_NAME, A.DATA_TYPE, A.DATA_PREFIX, A.DATA_LENGTH, A.DATA_VALUES, A.REGEX, A.BUSSINESS_MESSAGE, A.EXTRA_CHECK, A.DESCRIPTION FROM IMD_OFM_ATTRIBUTE_VALIDATE_DEF A
