                                                               setVariableData("ErrorTechMessage", validationCheck.split("###")[1].split("###-")[0]);     
                                                               setVariableData("ErrorTechDetails", validationCheck.split("###-")[1].split("#####")[0]);     
                                                           }     
                                                       }     
                                                        
              
                 } catch (Exception exc) {     
                     addAuditTrailEntry(exc);     
                     exc.printStackTrace();     
                 }]]>
