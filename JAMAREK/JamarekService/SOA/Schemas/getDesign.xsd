<?xml version="1.0" encoding="windows-1256" ?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:EBI="http://www.fact.com.jo"
            targetNamespace="http://www.fact.com.jo" elementFormDefault="qualified">

<xsd:element name="JIB" type="EBI:JIB"/>
<xsd:element name="Result" type="EBI:Result"/>

<xsd:complexType name="JIB">
    <xsd:sequence>
      <xsd:element name="BANK_USER" type="xsd:string"       minOccurs="1" maxOccurs="1"/>
      <xsd:element name="PASSWORD" type="xsd:string"        minOccurs="1" maxOccurs="1"/>
      <xsd:element name="GUARANTY_NO" type="xsd:string"     minOccurs="1" maxOccurs="1"/>
      <xsd:element name="BRANCH_NO" type="xsd:string"       minOccurs="1" maxOccurs="1"/>
      <xsd:element name="ACTION" type="xsd:string"          minOccurs="1" maxOccurs="1"/>
      <xsd:element name="STAMPS" type="xsd:string"          minOccurs="1" maxOccurs="1"/>
      <xsd:element name="NEW_VALUE" type="xsd:string"       minOccurs="1" maxOccurs="1"/>
      <xsd:element name="NEW_DATE" type="xsd:string"        minOccurs="1" maxOccurs="1"/>
      <xsd:element name="GUARANTEED_NAME" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <xsd:element name="PURPOSE" type="xsd:string"         minOccurs="1" maxOccurs="1"/>
      <xsd:element name="GUARANTY_VALUE" type="xsd:string"  minOccurs="1" maxOccurs="1"/>
      <xsd:element name="ISSUE_DATE" type="xsd:string"      minOccurs="1" maxOccurs="1"/>
      <xsd:element name="DUE_DATE" type="xsd:string"        minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
    </xsd:complexType>
    
    <xsd:complexType name="Result">
     <xsd:sequence>
      <xsd:element name="Code" type="xsd:string"  minOccurs="1" maxOccurs="1"/>
      <xsd:element name="Desc" type="xsd:string"      minOccurs="1" maxOccurs="1"/>
      <xsd:element name="Function" type="xsd:string"        minOccurs="1" maxOccurs="1"/>
    </xsd:sequence>
  </xsd:complexType>

   

</xsd:schema>
